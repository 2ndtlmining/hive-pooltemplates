[
    {
        "pool": {
            "name": "Hiveon",
            "url": "https://hiveon.net",
            "fee": 0,
            "type": "PPS+"
        }
    },
    {
        "coin": "ETH",
        "servers": [
            {
                "geo": "Russia",
                "urls": [
                    "ru-eth.hiveon.net:4444",
                    "ru-eth.hiveon.net:14444"
                ],
                "ssl_urls": [
                    "ru-eth.hiveon.net:24443"
                ]
            },
            {
                "geo": "Europe",
                "urls": [
                    "eu-eth.hiveon.net:4444",
                    "eu-eth.hiveon.net:14444"
                ],
                "ssl_urls": [
                    "eu-eth.hiveon.net:24443"
                ]
            },
            {
                "geo": "N.America West",
                "urls": [
                    "naw-eth.hiveon.net:4444",
                    "naw-eth.hiveon.net:14444"
                ],
                "ssl_urls": [
                    "naw-eth.hiveon.net:24443"
                ]
            },
            {
                "geo": "Asia",
                "urls": [
                    "aspac1-eth.hiveon.net:4444",
                    "aspac1-eth.hiveon.net:14444"
                ],
                "ssl_urls": [
                    "aspac1-eth.hiveon.net:24443"
                ]
            }
        ],
        "miners": {
            "_prototype": "miners_ethash",
            "tt-miner": {
                "url": "stratum1+tcp://%URL%"
            },
            "bminer": {
               "algo": "ethproxy"
            },
            "teamredminer":{
               "template": "%WAL%",
               "worker": "%WORKER_NAME%"
            },
            "lolminer":{
               "template": "%WAL%",
               "user_config": "--worker %WORKER_NAME%\n--ethstratum ETHPROXY"
            }
        }
    },
    {
        "coin": "ETC",
        "servers": [
            {
                "geo": "Russia",
                "urls": [
                    "ru-etc.hiveon.net:8888",
                    "ru-etc.hiveon.net:4444"
                ],
                "ssl_urls": [
                    "ru-etc.hiveon.net:20443"
                ]
            },
            {
                "geo": "Europe",
                "urls": [
                    "eu-etc.hiveon.net:8888",
                    "eu-etc.hiveon.net:4444"
                ],
                "ssl_urls": [
                    "eu-etc.hiveon.net:20443"
                ]
            },
            {
                "geo": "N.America",
                "urls": [
                    "na-etc.hiveon.net:8888",
                    "na-etc.hiveon.net:4444"
                ],
                "ssl_urls": [
                    "na-etc.hiveon.net:20443"
                ]
            },
            {
                "geo": "Asia-Pacific",
                "urls": [
                    "aspac-etc.hiveon.net:8888",
                    "aspac-etc.hiveon.net:4444"
                ],
                "ssl_urls": [
                    "aspac-etc.hiveon.net:20443"
                ]
            }
        ],
        "miners": {
            "ethminer": {
                "cuda": 1,
                "opencl": 1,
                "pass": "x",
                "port": "%URL_PORT%",
                "server": "stratum1+tcp://%URL_HOST%",
                "template": "%WAL%.%WORKER_NAME%"
            },
            "asicminer": {
	        "url": "stratum+tcp://%URL%",
                "template": "%WAL%.%WORKER_NAME%"
	    },
            "phoenixminer": {
                "url": "POOL: %URL%, WALLET: %WAL%.%WORKER_NAME%, PSW: x",
                "user_config": "-coin %COIN%"
            },
            "bminer": {
                "url": "%URL%",
                "algo": "ethproxy",
	        "template": "%WAL%.%WORKER_NAME%",
	        "pass": "x"
	    },
	    "nanominer": {
	        "algo": "ethash",
	        "url": "%URL%",
	        "template": "%WAL%",
	        "pass": "x",
	        "user_config": "coin = %COIN%\nrigName = %WORKER_NAME%"
	    },
	    "nbminer": {
	        "url": "%URL%",
	        "algo": "ethash",
	        "pass": "x",
	        "template": "%WAL%.%WORKER_NAME%"
	    },
	    "tt-miner": {
                "url": "stratum1+tcp://%URL%",
	        "algo": "ETHASH",
	        "pass": "x",
	        "template": "%WAL%.%WORKER_NAME%",
    	        "user_config": "-RH\n"
	    },
	    "gminer": {
	        "algo": "ethash",
	        "server": "%URL_HOST%",
	        "port": "%URL_PORT%",
	        "pass": "x",
	        "template": "%WAL%.%WORKER_NAME%"
	    },
	    "teamredminer": {
	        "algo": "ethash",
	        "url": "stratum+tcp://%URL%",
                "template": "%WAL%",
                "worker": "%WORKER_NAME%",
    		"pass": "x"
	    },
            "lolminer": {
                "algo": "ETHASH",
                "server": "%URL_HOST%",
                "port": "%URL_PORT%",
                "template": "%WAL%",
                "pass": "x",
                "user_config": "--worker %WORKER_NAME%\n--ethstratum ETHPROXY"
            },
            "srbminer": {
                "url": "%URL%",
                "algo": "ethash",
                "worker": "%WORKER_NAME%",
                "template": "%WAL%",
                "user_config": "--disable-cpu"
            },
	    "t-rex": {
		"url": "%URL%",
		"algo": "ethash",
		"template": "%WAL%",
		"user_config": "\"worker\": \"%WORKER_NAME%\""
	    }
        }
    }
]
